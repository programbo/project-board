  "description": "Meerkats project wheels",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./bin/www",
    "build": "webpack --progress",
    "deploy": "node ./docker/bin/deploy.js",
    "dev": "node ./bin/www",
    "public-sync": "if [ -d 'docker-public' ]; then rsync -avh docker-public/ public/ --delete; fi;",
    "docker-staging": "npm run public-sync && NODE_ENV=staging node ./bin/www",
    "docker-production": "npm run public-sync && NODE_ENV=production node ./bin/www",
    "test": "npm run lint",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx"
  },
